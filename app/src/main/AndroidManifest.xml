<?xml version="1.0" encoding="utf-8"?>
<manifest
	xmlns:android="http://schemas.android.com/apk/res/android"
  package="cburuel.stx.mx.testingws">

	<uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
	<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
	<uses-permission android:name="android.permission.INTERNET"/>
	<uses-permission android:name="android.permission.READ_PHONE_STATE"/>
	<uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

	<uses-permission android:name="android.permission.RECEIVE_SMS" android:required="false"/>
	<uses-permission android:name="android.permission.READ_SMS" android:required="false"/>

	<application
		android:allowBackup="true"
		android:icon="@drawable/ic_bug"
		android:label="@string/app_name"
		android:supportsRtl="true"
		android:theme="@style/AppTheme">
		<activity android:name=".Actividades.ActividadElegirEXT">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>

				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>

		<activity android:name=".Actividades.ActividadLogin"/>
		<activity android:name=".Actividades.ActividadVerificaCuenta"/>
		<activity android:name=".Actividades.ActividadPruebaWs"
		          android:windowSoftInputMode="adjustPan"/>
		<activity android:name=".Actividades.ActividadResultado"/>
		<activity android:name=".Actividades.AcvitidadImagen"/>

		<service android:name=".Servicios.BackendService"/>
	</application>
</manifest>